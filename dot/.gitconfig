[push]
	default = simple
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
[gc]
  auto=1
[merge]
  summary=true
[alias]
  s = status
  b = branch -v
  d = diff
  unstage = reset HEAD
  uncommit = reset --soft HEAD^
  lol = log --oneline --graph --decorate
